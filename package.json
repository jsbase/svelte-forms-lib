{
  "name": "svelte-form-lib",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "npm run format:stylelint && npm run format:prettier",
    "format:prettier": "prettier --ignore='./{build,public,dist,node_modules,.history,.lh,.git,.vscode,.cache}/**' --write .",
    "format:stylelint": "prettier-stylelint --ignore='./{build,public,dist,node_modules,.history,.lh,.git,.vscode,.cache}/**' --write .",
    "browserslist": "browserslist --coverage=DE,global --mobile-to-desktop --ignore-unknown-versions && browserslist --update-db"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "browserslist": "^4.19.3",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "prettier-stylelint": "^0.4.2",
    "stylelint": "^14.5.3",
    "svelte": "^3.44.0",
    "vite": "^2.8.0",
    "yup": "^0.32.11"
  },
  "dependencies": {
    "svelte-forms-lib": "^2.0.1"
  }
}
